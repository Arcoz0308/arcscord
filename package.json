{
  "name": "@arcscord/arcscord",
  "packageManager": "pnpm@9.12.1",
  "description": "A discord.js framework, slash command first and full typescript !",
  "author": "Arcoz0308",
  "license": "MIT",
  "homepage": "https://github.com/arcoz0308/arcscord#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcoz0308/arcscord.git"
  },
  "bugs": {
    "url": "https://github.com/arcoz0308/arcscord/issues"
  },
  "keywords": [
    "discord",
    "framework",
    "discord.js"
  ],
  "scripts": {
    "build": "pnpm -r --filter='./packages/**' run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "pnpm -r run typecheck",
    "test": "pnpm -r --filter='./packages/**' run test",
    "test:test-bot": "pnpm -r --filter '@arcscord/test-bot' run start",
    "docs:api": "typedoc",
    "docs:json": "pnpm -r run docs",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "only-allow": "^1.2.1",
    "tsc-alias": "^1.8.10",
    "typedoc": "^0.26.9",
    "typescript": "^5.3.3"
  }
}
