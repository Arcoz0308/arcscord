{
  "name": "arcscord",
  "version": "0.1.16",
  "description": "A discord.js framework, slash command first and full typescript !",
  "keywords": [
    "discord",
    "framework",
    "discord.js"
  ],
  "author": "Arcoz0308",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test:simpleCommand": "npx nodemon --exec node -r @swc-node/register -r tsconfig-paths/register ./test/simple_bot/index.ts",
    "test": "vitest"
  },
  "directories": {
    "lib": "./src",
    "test": "./test"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@arcscord/better-error": "^1.0.3",
    "@arcscord/error": "^1.0.1",
    "@discordjs/builders": "^1.7.0",
    "cron": "^3.1.6",
    "dayjs": "^1.11.10",
    "discord-api-types": "^0.37.70",
    "discord.js": "^14.14.1",
    "short-unique-id": "^5.0.3",
    "tintify": "^1.1.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@arcoz/eslint-config": "^1.1.1",
    "@swc-node/core": "^1.13.0",
    "@swc-node/register": "^1.9.0",
    "@types/node": "^20.11.24",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "nodemon": "^3.1.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.1.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcoz0308/arcscord.git"
  },
  "bugs": {
    "url": "https://github.com/arcoz0308/arcscord/issues"
