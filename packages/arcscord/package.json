{
  "name": "arcscord",
  "version": "0.2.0",
  "description": "A discord.js framework, slash command first and full typescript !",
  "author": "Arcoz0308",
  "license": "MIT",
  "homepage": "https://github.com/arcscord/arcscord#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcscord/arcscord.git"
  },
  "bugs": {
    "url": "https://github.com/arcscord/arcscord/issues"
  },
  "keywords": [
    "discord",
    "framework",
    "discord.js",
    "arcscord",
    "typescript",
    "slash-commands",
    "typed-commands"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/esm/index.d.mts",
      "default": "./dist/esm/index.mjs"
    },
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "imports": {
    "#/*": "./dist/*.js"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "typecheck": "tsc -b src",
    "docs": "typedoc"
  },
  "dependencies": {
    "@arcscord/better-error": "^1.0.4",
    "@arcscord/error": "^2.0.1",
    "cron": "^3.2.1",
    "dayjs": "^1.11.13",
    "discord-api-types": "^0.37.110",
    "discord.js": "^14.16.3",
    "i18next": "^23.16.8",
    "tintify": "^1.1.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@swc-node/core": "^1.13.3",
    "@swc-node/register": "^1.10.9",
    "@types/node": "^22.10.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.5",
    "typedoc": "^0.26.11",
    "vite": "5.4.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist/"
  ]
}
