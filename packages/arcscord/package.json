{
  "name": "arcscord",
  "version": "0.2.0",
  "description": "A discord.js framework, slash command first and full typescript !",
  "author": "Arcoz0308",
  "license": "MIT",
  "homepage": "https://github.com/arcoz0308/arcscord#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcoz0308/arcscord.git"
  },
  "bugs": {
    "url": "https://github.com/arcoz0308/arcscord/issues"
  },
  "keywords": [
    "discord",
    "framework",
    "discord.js",
    "arcscord",
    "typescript",
    "slash-commands",
    "typed-commands"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "typecheck": "tsc --project tsconfig.typecheck.json && tsc-alias -p tsconfig.typecheck.json",
    "docs": "typedoc"
  },
  "directories": {
    "lib": "./src",
    "test": "../../test"
  },
  "dependencies": {
    "@arcscord/better-error": "workspace:*",
    "@arcscord/error": "workspace:*",
    "cron": "^3.1.7",
    "dayjs": "^1.11.13",
    "discord-api-types": "^0.37.97",
    "discord.js": "^14.16.3",
    "tintify": "^1.1.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@swc-node/core": "^1.13.3",
    "@swc-node/register": "^1.10.9",
    "@types/node": "^20.16.11",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.0",
    "typedoc": "^0.26.9",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
