{
  "name": "@arcscord/middleware",
  "version": "1.0.0",
  "description": "Middleware for arcscord",
  "author": "Arcoz0308",
  "license": "MIT",
  "homepage": "https://github.com/Arcoz0308/arcscord/tree/main/packages/middleware#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcoz0308/arcscord.git"
  },
  "bugs": {
    "url": "https://github.com/arcoz0308/arcscord/issues"
  },
  "keywords": [
    "arcscord",
    "discord",
    "middleware"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/esm/index.d.mts",
      "default": "./dist/esm/index.mjs"
    },
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --project tsconfig.typecheck.json && tsc-alias -p tsconfig.typecheck.json",
    "docs": "typedoc"
  },
  "dependencies": {
    "arcscord": "workspace:^",
    "discord.js": "^14.16.3"
  },
  "devDependencies": {
    "tsup": "^8.3.0"
  },
  "directories": {
    "lib": "./src"
  }
}
