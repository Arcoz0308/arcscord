{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "#/*": [
        "./src/*"
      ]
    },
    "resolveJsonModule": true,
    "outDir": "./out",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true
  }
}